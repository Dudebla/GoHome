        <activity android:name=".User.Activity.UserAdoptProActivity" />
        <activity android:name=".User.Activity.UserPersonalInforActivity" />
        <activity android:name=".User.Activity.UserAdoptActivity" />
        <activity android:name=".User.Activity.UserReportActivity" />
        <activity android:name=".User.Activity.UserSendActivity" />
        <activity android:name=".Organizer.OrganizerMain" />
        <activity android:name=".User.Activity.UserHomeActivity" />
        <activity android:name=".Member.Activity.MemberHomeActivity"/>
        <activity android:name=".Member.Activity.MemberHandleOperationActivity"/>
        <activity android:name=".Register.RegisterActivity">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>